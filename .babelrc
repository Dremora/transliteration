{
  "presets": [
      ["@babel/preset-env", {
          "loose": true,
          "spec": true,
          "targets": {
              "browsers": [
                  "last 3 Chrome versions",
                  "last 3 Firefox versions",
                  "Safari >= 7",
                  "Explorer >= 10",
                  "Edge >= 12",
                  "iOS >= 10",
                  "Android >= 6"
              ]
          },
          "useBuiltIns": "usage",
          "debug": false
      }]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "comments": false
}
